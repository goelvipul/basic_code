i=0
def sorting_fun(list1):
    '''

       Objective :to sort the list
       input parameters : list
       output : sorted list

    '''
    minm = list1[i]
    for i in range(len(list1)) :
         if (minm > list1[i+1]) :
             temp = minm
             minm = list1[i+1]
             list1[i+1] = temp
         i=i+1
    
