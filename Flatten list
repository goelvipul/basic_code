tmp=0
def sort_fun(list1):
    '''
       Objective :to sort the list
       input parameters : list
       output : sorted list
    '''
    min=list1[tmp]
    for tmp in range(len(list1)) :
         if (min>list1[tmp+1]) :
             temp = min
             min = list1[tmp+1]
             list1[tmp+1] = temp
         tmp = tmp+1
    
